   ],
   "source": [
    "from scipy.stats import fisher_exact\n",
    "\n",
    "# The contingency table\n",
    "table = [[15, 16], [12, 19]]\n",
    "\n",
    "# Perform Fisher's exact test\n",
    "oddsratio, pvalue = fisher_exact(table)\n",
    "\n",
    "print(\"Odds Ratio:\", oddsratio)\n",
    "print(\"P-value:\", pvalue)\n"
   ]
